\begin{algorithm}[h]
    \caption{Test Algorithm}
    \begin{algorithmic}[1] % The number tells where the line numbering should start
            \STATE $i\gets 10$
            \IF {$i\geq 5$} 
              \STATE $i\gets i-1$
            \ELSE
              \IF {$i\leq 3$}
                \STATE $i\gets i+2$
              \ENDIF
            \ENDIF 

    \end{algorithmic}
\end{algorithm}
